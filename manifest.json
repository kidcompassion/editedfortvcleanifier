
{
  "manifest_version": 2,

  "name": "TBS Cleanifier",
  "version": "1",
  "description": "The only Chrome Extension with the guts to clean up the internet",
  "browser_action": {
    "name": "Manipulate DOM",
    "icons": ["icon.png"],
    "default_icon": "icon.png"
  },
  "content_scripts": [ {
    "js": [ "jquery-1.10.2.min.js", "background.js" ],
    "matches": [ "http://*/*", "https://*/*"]
  }]
}